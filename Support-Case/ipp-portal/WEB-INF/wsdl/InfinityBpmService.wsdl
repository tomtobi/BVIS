         <wsdl:documentation>
         Retrieves (parts of) the worklist of the current logged in user satisfying the criteria specified in the provided query. 
         </wsdl:documentation>
         <wsdl:input message="impl:findWorklistRequest" />
         <wsdl:output message="impl:findWorklistResponse" />
         <wsdl:fault message="impl:bpmFault" name="fault" />
      </wsdl:operation>

      <wsdl:operation name="activateActivity" parameterOrder="parameters">
         <wsdl:documentation>
