         <wsdl:fault message="impl:bpmFault" name="fault" />
      </wsdl:operation>

      <wsdl:operation name="getActivity" parameterOrder="parameters">
         <wsdl:documentation>
         Retrieves the specified activity instance.     
         </wsdl:documentation>
         <wsdl:input message="impl:getActivityRequest" />
